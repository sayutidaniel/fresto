{
  "name": "fresto",
  "version": "0.0.1",
  "description": "A web app for helping people to find restaurants",
  "main": "lib/server.js",
  "scripts": {
    "clean": "rimraf lib/*",
    "coverage": "npm test -- --coverage",
    "compile:client": "webpack --config webpack.client.js",
    "compile:server": "webpack --config webpack.server.js",
    "build": "npm run compile:server -- --bail && npm run compile:client -- --bail",
    "build:watch": "npm run compile:server -- --watch",
    "lint": "eslint src/**",
    "lint:fix": "npm run lint -- --fix",
    "start": "node .",
    "test": "jest",
    "watch": "nodemon lib/server.js"
  },
  "author": "Sayuti Daniel",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "config": "^1.21.0",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.15.0",
    "mongoose": "^4.6.0",
    "morgan": "^1.7.0",
    "oauth": "^0.9.14",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "codecov": "^1.0.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jest": "^15.1.1",
    "nodemon": "^1.10.2",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^0.13.0",
    "react-test-renderer": "^15.3.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2"
  }
}
